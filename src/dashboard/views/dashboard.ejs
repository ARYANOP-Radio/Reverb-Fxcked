                </div>
                <div class="w-layout-grid grid-solution">
                    <% if (user && user.id) { %>
                      <% user.guilds.filter(guild=>{
                        const permsOnGuild = new Permissions(guild.permissions_new);
                        if(permsOnGuild.has(Permissions.Flags.ManageGuild)) return guild;
                      }).sort((a, b) => {
                        return (client.guilds.cache.get(a.id) == client.guilds.cache.get(b.id)) ? 0 : a ? -1 : 1;
                      }).forEach(guild => {
                      %>
                        <div id="w-node-b95fd48d-5503-cf62-e443-27c3e4b6a7bb-4bc71e26" data-w-id="b95fd48d-5503-cf62-e443-27c3e4b6a7bb" style="opacity:0" class="solution">
                            <div class="icon-solution-block">
                              <%- guild.icon ? `<img src="https://cdn.discordapp.com/icons/${guild.id}/${guild.icon}" loading="eager" alt="" style="border-radius: 50%;" class="icon-solution" />` : `<img src="https://media.discordapp.net/attachments/936524382501765140/986967642336272474/unknown.png?width=427&height=427" loading="eager" alt="" style="border-radius: 50%;" class="icon-solution" />` %>
                              <div class="icon-solution-blur"></div>
                            </div>
                            <% if (client.guilds.cache.get(guild.id)) { %>
                              <div class="solution-block">
                                <h6 class="heading-solution"><%= guild.name %></h6>
                                <br>
                                <a href="/dashboard/<%- guild.id %>" class="nav-button w-button">Edit Settings<br/></a>
                              </div>
                            <% } else { %>
                              <div class="solution-block">
                                <h6 class="heading-solution"><%= guild.name %></h6>
                                <br>
                                <a href="<%= `https://discordapp.com/oauth2/authorize?client_id=${client.user.id}&permissions=139891961200&scope=bot%20applications.commands&guild_id=${guild.id}&response_type=code&redirect_uri=${encodeURIComponent(`${client.config.Website}/dashboard`)}`  %>" class="nav-button w-button">Invite Reverb<br/></a>
                              </div>
                            <% } %>
                        </div>
                      <% }) %>
                    <% } %>
